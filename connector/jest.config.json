{
    "transform": {
        "^.+\\.tsx?$": "ts-jest"
    },
    "collectCoverage": false,
    "collectCoverageFrom": [
        "src/connector/**/*.{ts,tsx}",
        "src/proxy/**/*.{ts,tsx}",
        "!**/*.d.ts"
    ],
    "testRegex": "(/src/.+/.*\\.test)\\.(tsx?)$",
    "moduleFileExtensions": [
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "node"
    ],
    "setupFiles": [
        "./jest.setup.js"
    ]
}